<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_map">

    <fragment
        android:id="@+id/nav_map"
        android:name="com.nickjgski.vtjoinme.ui.map.MapFragment"
        android:label="@string/menu_map" />

    <fragment
        android:id="@+id/nav_list"
        android:name="com.nickjgski.vtjoinme.ui.list.ListFragment"
        android:label="@string/menu_list"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/nav_friends"
        android:name="com.nickjgski.vtjoinme.ui.friends.FriendsFragment"
        android:label="@string/menu_friends"
        tools:layout="@layout/fragment_friends" />

    <fragment
        android:id="@+id/nav_account"
        android:name="com.nickjgski.vtjoinme.ui.account.AccountFragment"
        android:label="@string/menu_account"
        tools:layout="@layout/fragment_tools" />
    <fragment
        android:id="@+id/nav_add"
        android:name="com.nickjgski.vtjoinme.AddFragment"
        android:label="@string/menu_add"
        tools:layout="@layout/fragment_add" />

</navigation>